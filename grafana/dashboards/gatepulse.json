{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" }
  ],
  "annotations": { "list": [] },
  "description": "GatePulse webhook delivery metrics",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "type": "stat",
      "title": "Events Published (rate 5m)",
      "id": 1,
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(rate(gatepulse_events_published_total[5m]))",
          "legendFormat": "events/s",
          "refId": "A"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "orientation": "auto", "textMode": "auto", "colorMode": "background", "graphMode": "area" },
      "fieldConfig": { "defaults": { "unit": "reqps", "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "blue", "value": null }] } }, "overrides": [] }
    },
    {
      "type": "stat",
      "title": "Queue Depth",
      "id": 2,
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(gatepulse_queue_depth)",
          "legendFormat": "pending jobs",
          "refId": "A"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "orientation": "auto", "textMode": "auto", "colorMode": "background", "graphMode": "area" },
      "fieldConfig": { "defaults": { "unit": "short", "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 50000 }, { "color": "red", "value": 90000 }] } }, "overrides": [] }
    },
    {
      "type": "stat",
      "title": "DLQ Total",
      "id": 3,
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(gatepulse_dlq_total)",
          "legendFormat": "dlq jobs",
          "refId": "A"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "orientation": "auto", "textMode": "auto", "colorMode": "background", "graphMode": "none" },
      "fieldConfig": { "defaults": { "unit": "short", "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "red", "value": 1 }] } }, "overrides": [] }
    },
    {
      "type": "stat",
      "title": "SSE Clients",
      "id": 4,
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "gatepulse_stream_clients",
          "legendFormat": "clients",
          "refId": "A"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "orientation": "auto", "textMode": "auto", "colorMode": "value", "graphMode": "area" },
      "fieldConfig": { "defaults": { "unit": "short", "color": { "mode": "fixed", "fixedColor": "purple" } }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "Delivery Throughput",
      "id": 5,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "sum(rate(gatepulse_deliveries_total{status=\"success\"}[5m]))", "legendFormat": "success", "refId": "A" },
        { "expr": "sum(rate(gatepulse_deliveries_total{status=\"retried\"}[5m]))", "legendFormat": "retried", "refId": "B" },
        { "expr": "sum(rate(gatepulse_deliveries_total{status=\"failed\"}[5m]))", "legendFormat": "failed", "refId": "C" }
      ],
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "fieldConfig": {
        "defaults": { "unit": "reqps", "custom": { "lineWidth": 2, "fillOpacity": 10 } },
        "overrides": [
          { "matcher": { "id": "byName", "options": "success" }, "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "green" } }] },
          { "matcher": { "id": "byName", "options": "retried" }, "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "yellow" } }] },
          { "matcher": { "id": "byName", "options": "failed" }, "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }] }
        ]
      }
    },
    {
      "type": "timeseries",
      "title": "Delivery Latency",
      "id": 6,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "histogram_quantile(0.50, sum(rate(gatepulse_delivery_latency_ms_bucket[5m])) by (le))", "legendFormat": "P50", "refId": "A" },
        { "expr": "histogram_quantile(0.95, sum(rate(gatepulse_delivery_latency_ms_bucket[5m])) by (le))", "legendFormat": "P95", "refId": "B" },
        { "expr": "histogram_quantile(0.99, sum(rate(gatepulse_delivery_latency_ms_bucket[5m])) by (le))", "legendFormat": "P99", "refId": "C" }
      ],
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "fieldConfig": { "defaults": { "unit": "ms", "custom": { "lineWidth": 2, "fillOpacity": 5 } }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "In-Flight Deliveries",
      "id": 7,
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "sum(gatepulse_inflight)", "legendFormat": "inflight", "refId": "A" }
      ],
      "options": { "tooltip": { "mode": "single" } },
      "fieldConfig": { "defaults": { "unit": "short", "color": { "mode": "fixed", "fixedColor": "blue" }, "custom": { "lineWidth": 2, "fillOpacity": 20 } }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "Overload Drops",
      "id": 8,
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "sum(rate(gatepulse_overload_drops_total[5m]))", "legendFormat": "drops/s", "refId": "A" }
      ],
      "options": { "tooltip": { "mode": "single" } },
      "fieldConfig": { "defaults": { "unit": "reqps", "color": { "mode": "fixed", "fixedColor": "red" }, "custom": { "lineWidth": 2, "fillOpacity": 20 } }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "Store IPC Operations",
      "id": 9,
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "sum by (op, status) (rate(gatepulse_store_ops_total[5m]))", "legendFormat": "{{op}} {{status}}", "refId": "A" }
      ],
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "fieldConfig": { "defaults": { "unit": "reqps", "custom": { "lineWidth": 1, "fillOpacity": 5 } }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "Rate Limited Deliveries",
      "id": 10,
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 18 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "sum(rate(gatepulse_rate_limited_total[5m]))", "legendFormat": "rate limited/s", "refId": "A" }
      ],
      "options": { "tooltip": { "mode": "single" } },
      "fieldConfig": { "defaults": { "unit": "reqps", "color": { "mode": "fixed", "fixedColor": "orange" }, "custom": { "lineWidth": 2, "fillOpacity": 10 } }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "SSE Stream Events Sent",
      "id": 11,
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 18 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "sum(rate(gatepulse_stream_events_total[5m]))", "legendFormat": "events/s", "refId": "A" }
      ],
      "options": { "tooltip": { "mode": "single" } },
      "fieldConfig": { "defaults": { "unit": "reqps", "color": { "mode": "fixed", "fixedColor": "purple" }, "custom": { "lineWidth": 2, "fillOpacity": 10 } }, "overrides": [] }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "tags": ["gatepulse", "webhooks"],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "pluginId": "prometheus",
        "label": "Prometheus",
        "hide": 0,
        "current": {}
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "GatePulse Webhook Delivery",
  "uid": "gatepulse-main",
  "version": 1
}
